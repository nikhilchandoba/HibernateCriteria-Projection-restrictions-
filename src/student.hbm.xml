<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="Student" table="std3">  
    <id name="id">  
     <generator class="identity"></generator>  
    </id>  
            
    <property name="Fname" column="std_fn"></property>  
    <property name="Sname" column="std_ln"></property> 
    <property name="marks" column="std_mark"></property> 
   
            
  </class>  
            
 </hibernate-mapping>  